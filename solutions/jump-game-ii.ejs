 /**
 * @param {number[]} nums
 * @return {number}
 */
 var jump = function(nums) {
     var length = nums.length;
     var step = 0;
     var start = 0;
     var end = 0;
     while (end < length - 1) {
         step++;
         var maxend = end + 1;
         for (var i = start; i <= end; i++) {
             if (i + nums[i] >= length - 1) return step;
             maxend = max(maxend, i + nums[i]);
         }
         start = end + 1;
         end = maxend;
     }
     return step;
 };
 var max = function(a,b){
     return a>=b?a:b;
 }
